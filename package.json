{
  "name": "@soulofmischief/js-utils",
  "version": "0.18.6",
  "description": "A small, utilitarian JS library for personal use.",
  "author": "soulofmischief <soulofmischief@protonmail.com>",
  "repository": "https://github.com/soulofmischief/js-utils",
  "license": "MIT",
  "engines": {
    "node": ">=8.5.0"
  },
  "main": "index",
  "scripts": {
    "build": "npm run clean --scripts-prepend-node-path && yarn build:flow && yarn build:lib",
    "build:dev": "NODE_ENV=development nodemon -w src --exec 'yarn build'",
    "build:flow": "yarn flow-copy-source src lib",
    "build:lib": "babel src -d lib",
    "clean": "yarn rimraf lib",
    "lint:flow": "yarn flow status",
    "lint:flow:stop": "yarn flow stop",
    "publish:npm": "./scripts/publish.sh",
    "test": "babel-node $(yarn bin ospec)"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-function-bind": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-flow": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^5.9.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "flow-bin": "^0.159.0",
    "flow-copy-source": "^2.0.9",
    "ospec": "^4.1.1",
    "rimraf": "^3.0.2",
    "yarn": "^1.22.11"
  },
  "dependencies": {
    "@soulofmischief/array.js": "^0.19.5",
    "@soulofmischief/async.js": "^5.0.0",
    "lodash": "^4.17.21"
  }
}
